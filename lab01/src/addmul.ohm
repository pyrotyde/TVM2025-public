AddMul {
    Expr = AddExp
    AddExp = AddExp "+" MulExp -- plus
           | MulExp
    MulExp = MulExp "*" ParExp --  times
           | ParExp 
    ParExp = "(" AddExp ")"  -- parPlus
           | "(" MulExp ")" -- parMult
           | "(" number ")"  -- parNum
           | number
    number = digit+
}